    <file src="..\..\Samples\DualStickSpaceShooter\Content\Plugins\DualStickSpaceShooter.core.xml" target="lib" />
    <file src="..\..\Samples\DualStickSpaceShooter\Content\Data\**\*.res" target="content" />
    <file src="..\..\Samples\DualStickSpaceShooter\**\*.cs" target="source" exclude="..\..\Samples\DualStickSpaceShooter\Content\**\*.*;..\..\Samples\DualStickSpaceShooter\obj\**\*.*" />
    <file src="..\..\Samples\DualStickSpaceShooter\DualStickSpaceShooter.csproj" target="source" />
